{
  "version": 2,
  // Force redeploy with pagination fixes
  "builds": [
    {
      "src": "dist/public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/public/assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
}
